{
  "name": "easyshell",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "docker compose -f ./dev/compose.yml up",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio",
    "next:build": "bunx next build",
    "next:dev": "bunx next dev",
    "next:lint": "bunx next lint",
    "next:start": "bunx next start",
    "problems:lint": "bun tools/problems/lint",
    "problems:build": "DATABASE_URL='http://random' bun tools/problems/build",
    "queue-processor:dev": "bun --watch tools/queue-processor",
    "queue-processor:start": "bun tools/queue-processor",
    "container-manager:up": "docker compose up -f tools/container-manager/docker-compose.yml",
    "container-manager:lint": "golangci-lint run tools/container-manager/main.go",
    "container-manager:down": "docker compose down -f tools/container-manager/docker-compose.yml",
    "container-io:lint": "golangci-lint run tools/problems/easyshell-base/container-io/main.go",
    "submission-runner:lint": "golangci-lint run tools/problems/easyshell-base/submission-runner/main.go",
    "prettier:check": "bunx prettier --check .",
    "prettier:write": "bunx prettier --write .",
    "lint": "bun problems:lint && bun next:lint && bun prettier:check && bun container-manager:lint && bun container-io:lint && bun submission-runner:lint"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.4",
