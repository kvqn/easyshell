  "devDependencies": {
    "@types/bun": "^1.2.2",
    "@types/eslint": "^8.56.12",
    "@types/mdx": "^2.0.13",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.17.19",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/utf8": "^3.0.3",
